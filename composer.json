{
  "name": "swow-cloud/rate-limit",
  "description": "General purpose rate limiter implementation.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "rate limit",
    "redis"
  ],
  "authors": [
    {
      "name": "heping",
      "email": "84705412@qq.com"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^8.0",
    "beberlei/assert": "^3.2"
  },
  "require-dev": {
    "ext-apcu": ">=5.1.12",
    "ext-redis": "*",
    "friendsofphp/php-cs-fixer": "^2.17",
    "phpstan/phpstan": "^0.12.10",
    "phpstan/phpstan-beberlei-assert": "^0.12.2",
    "phpstan/phpstan-phpunit": "^0.12.6",
    "phpunit/phpunit": "^8.0",
    "predis/predis": "^1.1"
  },
  "suggest": {
    "ext-apcu": "In order to use ApcuRateLimiter",
    "ext-memcached": "In order to use MemcachedRateLimiter",
    "ext-redis": "In order to use RedisRateLimiter",
    "predis/predis": "In order to use PredisRateLimiter"
  },
  "autoload": {
    "psr-4": {
      "SwowCloud\\RateLimit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SwowCloud\\RateLimit\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit --colors=always",
    "cs-fix": "php-cs-fixer fix --config=.php_cs",
    "stan": "phpstan analyse"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "version": "dev-master"
}
